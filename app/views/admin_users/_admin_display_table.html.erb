  <table summary="User detail view">
    <tr>
      <th>Username</th>
      <td><%= @admin_user.username %></td>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @admin_user.name %></td>
    </tr>
    <tr>
      <th>E-mail</th>
      <td><%= @admin_user.email %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @admin_user.created_at.to_s(:long) %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @admin_user.updated_at.to_s(:long) %></td>
    </tr>
  </table>
